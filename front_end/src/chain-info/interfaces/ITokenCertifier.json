  "source": "// SPDX-License-Identifier: MIT\n// Beef MarketPlace Contracts v0.0.0 hello@mcdu.com\n\npragma solidity ^0.8.0;\nimport \"./ITokenWithId.sol\";\n\n\ninterface ITokenCertifier is IERC165 {\n    function supportsInterface(bytes4 interfaceId) external view returns (bool);\n\n    function certify(\n        ITokenCertified token,\n        uint256 tokenId\n    )\n        external\n        view\n        returns (bool);\n}\n\ninterface ITokenCertified is IERC165 {\n    function supportsInterface(bytes4 interfaceId) external view returns (bool);\n\n    // who certify \n    function getCertifier() external view returns (ITokenCertifier);\n}\n",
  "type": "interface"
