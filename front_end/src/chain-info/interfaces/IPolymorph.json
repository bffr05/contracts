  "source": "// SPDX-License-Identifier: MIT\n// Beef MarketPlace Contracts v0.0.0 hello@mcdu.com\n\npragma solidity ^0.8.0;\nimport \"@openzeppelin/contracts/utils/introspection/IERC165.sol\";\n\ninterface IPolymorph is IERC165 {\n\n    function supportsInterface(bytes4 interfaceId) external view returns (bool);\n\n    //function transferFrom(address sender,address recipient,uint tokenid_,uint256 amount) external returns (bool);\n    //function transfer(address recipient,uint tokenid_, uint256 amount) external returns (bool);\n\n    function getAliasAddress() external view returns (address vtokenaddr_);\n    function getAliasTokenId(uint tokenid_) external view returns (uint vtokenid_);\n    //function getVirtualName() external view returns (string memory);\n    //function getVirtualSymbol() external view returns (string memory);\n\n    //function getTokenURI(uint256 tokenId) external view returns (string memory);\n\n    function isTradeable() external view returns (bool); \n    function getNFTQty() external view returns (uint256);\n    function isNFT() external view returns (bool); \n    function isTradeableOnlyWithCurrency() external view returns (bool); // is this token a currency, iscan be trade with any currency, else tradeable only with Currency(aka ETH)\n}",
  "type": "interface"
